<h1>Listing column_lists</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @column_lists.each do |column_list| %>
      <tr>
        <td><%= link_to 'Edit',  edit_list_column_list_path(@list, column_list) %></td>
        <td><%= link_to 'Destroy',  list_column_list_path(@list, column_list), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Column list',  new_list_column_list_path(@list) %>
