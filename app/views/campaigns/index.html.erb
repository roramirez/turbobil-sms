
<div class="row">
  <div class="col-lg-12">
     <h1 class="page-header">
       Campaigns
       <div class="pull-right">
        <%= link_to 'New Campaign', new_campaign_path, class: 'btn btn-default' %>
        </div>
     </h1>
  </div>
</div>

<div class="row">
  <div class="col-lg-12">

    <div class="panel panel-default">

      <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover" id="dataTable">
          <thead>
            <tr>
              <th colspan="3"></th>
            </tr>
          </thead>

          <tbody>
            <% @campaigns.each do |campaign| %>
            <tr>
              <td><%= link_to 'Edit', edit_campaign_path(campaign) %></td>
            </tr>
            <% end %>
         </tbody>
      </table>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-sm-12">
    <%#= paginate @campaigns, theme:"turbobil" %>
   </div>
</div>
<br>


