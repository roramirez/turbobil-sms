<div class="row">
  <div class="col-lg-12">
     <h1 class="page-header">
       <%= I18n.t('views.pages.lists') %>
       <div class="pull-right">
        <%= link_to I18n.t('views.buttons.new_list'), new_list_path, class: 'btn btn-default' %>
        </div>
     </h1>
  </div>
</div>

<div class="row">
  <div class="col-lg-12">

    <div class="panel panel-default">

      <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover" id="dataTable">
          <thead>
            <tr role="row">
              <th><%=  link_to_sorted I18n.t('activerecord.attributes.list.name') , :name %></th>
              <th><%=  I18n.t('views.buttons.operation') %></th>
            </tr>
          </thead>

          <tbody>
            <tr class="odd gradeX">
            <% @lists.each do |list| %>
              <td><%= list.name %></td>
              <td>
                 <%= link_to I18n.t('views.buttons.edit'), edit_list_path(list) %> |
                 <%= link_to I18n.t('views.buttons.import'), import_list_path(list) %>
              </td>
            </tr>
            <% end %>
         </tbody>
      </table>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-sm-12">
    <%= paginate @lists, theme:"turbobil" %>
   </div>
</div>
